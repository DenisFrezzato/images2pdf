{
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-config-prettier",
    "tslint-eslint-rules",
    "typestrict"
  ],
  "rules": {
    "interface-name": false,
    "object-literal-sort-keys": false,
    "curly": [true, "ignore-same-line"],
    "member-ordering": false,
    "no-floating-promises": false,

    // ESlint rules
    "ter-no-self-compare": true,
    "arrow-parens": true
  }
}
